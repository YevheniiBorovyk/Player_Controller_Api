<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" parallel="tests" thread-count="3">

    <test name="[SN] Create player">
        <classes>
            <class name="com.playercontroller.CreatePlayerTests"/>
        </classes>
    </test>
    <test name="[SN] Create player negative">
        <classes>
            <class name="com.playercontroller.GetPlayerByPlayerIdNegativeTests"/>
        </classes>
    </test>
    <test name="[SN] Delete player negative">
        <classes>
            <class name="com.playercontroller.DeletePlayerNegativeTests"/>
        </classes>
    </test>
    <test name="[SN] Get all players">
        <classes>
            <class name="com.playercontroller.GetAllPlayersTests"/>
        </classes>
    </test>
    <test name="[SN] Get player by player id">
        <classes>
            <class name="com.playercontroller.GetPlayerByPlayerIdTests"/>
        </classes>
    </test>
    <test name="[SN] Get player by player id negative">
        <classes>
            <class name="com.playercontroller.GetPlayerByPlayerIdNegativeTests"/>
        </classes>
    </test>
    <test name="[SN] Update player">
        <classes>
            <class name="com.playercontroller.UpdatePlayerTests"/>
        </classes>
    </test>
    <test name="[SN] Update player negative">
        <classes>
            <class name="com.playercontroller.UpdatePlayerNegativeTests"/>
        </classes>
    </test>

</suite>
