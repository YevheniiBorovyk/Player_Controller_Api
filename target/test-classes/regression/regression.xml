<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" parallel="tests" thread-count="1">

    <test name="Create article">
        <classes>
            <class name="com.bondaracademy.CreateArticle"/>
        </classes>
    </test>
    <test name="Change user bio">
        <classes>
            <class name="com.bondaracademy.ChangeUserBio"/>
        </classes>
    </test>
    <test name="Select and delete article as favorite">
        <classes>
            <class name="com.bondaracademy.SelectAndDeleteArticleAsFavorite"/>
        </classes>
    </test>

</suite>
